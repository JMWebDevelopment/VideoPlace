				},
				{
					"colors": [ "#9b8b6d","#f0f0f1" ],
					"slug": "primary-foreground",
					"name": "Blue and white"
				}
			]
		},
    "spacing": {
			"units": [ "px", "em", "rem", "vh", "vw","%" ]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "\"Playfair Display\", serif",
          "slug": "playfair-display",
          "name": "Playfair Display",
          "fontFace": [
            {
              "fontFamily": "Playfair Display",
              "fontWeight": "400 700",
              "fontStyle": "normal",
              "src": [ "file:./assets/webfonts/PlayfairDisplay-VariableFont_wght.ttf" ]
            },
            {
              "fontFamily": "Playfair Display",
              "fontWeight": "400 700",
              "fontStyle": "italic",
              "src": [ "file:./assets/webfonts/PlayfairDisplay-Italic-VariableFont_wght.ttf" ]
            }
          ]
        },
        {
          "fontFamily": "\"Roboto\", sans-serif",
          "slug": "roboto",
          "name": "Roboto",
          "fontFace": [
            {
              "fontFamily": "Roboto",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": [ "file:./assets/webfonts/Roboto-Regular.ttf" ]
            },
            {
              "fontFamily": "Roboto",
              "fontWeight": "700",
              "fontStyle": "normal",
              "src": [ "file:./assets/webfonts/Roboto-Bold.ttf" ]
            },
            {
              "fontFamily": "Roboto",
              "fontWeight": "400",
              "fontStyle": "italic",
              "src": [ "file:./assets/webfonts/Roboto-Italic.ttf" ]
