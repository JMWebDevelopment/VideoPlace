            }
          }
        },
        "core/site-tagline": {
          "spacing": {
            "margin": {
              "bottom": "20px"
            }
          },
          "typography": {
            "fontSize": "var(--wp--preset--font-size--max-30)",
            "lineHeight": "var(--wp--custom--line-height--heading)"
          }
        },
        "core/site-title": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--max-50)",
            "fontWeight": "var(--wp--custom--font-weight--bold)",
            "lineHeight": "var(--wp--custom--line-height--heading)",
            "fontFamily": "var(--wp--preset--font-family--roboto-slab)"
          }
        },
        "core/table": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--regular)"
          }
        },
        "core/template-part": {
          "spacing": {
            "margin": {
              "top": "0px"
            }
          }
        }
      }
    }
  }
