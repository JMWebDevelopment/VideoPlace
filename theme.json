        {
          "fontFamily": "\"Roboto Slab\", serif",
          "slug": "roboto-slab",
          "name": "Roboto Slab",
          "fontFace": [
            {
              "fontFamily": "Roboto Slab",
              "fontWeight": "400 700",
              "fontStyle": "normal",
              "src": [ "file:./assets/webfonts/RobotoSlab-VariableFont_wght.ttf" ]
            }
          ]
        }
      ],
      "fontSizes":[
				{
					"name": "Small",
					"slug": "small",
					"size": "16px"
				},
				{
					"name": "Regular",
					"slug": "regular",
					"size": "18px"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "20px"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "24px"
				},
				{
					"name": "xLarge",
					"slug": "x-large",
					"size": "28px"
				},
				{
					"name": "35px",
					"slug": "max-30",
					"size": "clamp(30px, 3vw, 35px)"
				},
				{
					"name": "40px",
					"slug": "max-40",
					"size": "clamp(35px, 4vw, 40px)"
				},
				{
					"name": "50px",
					"slug": "max-50",
					"size": "clamp(40px, 5vw, 50px)"
				}
			]
    },
    "custom": {
			"h-spacing": "2.375rem",
			"v-spacing": "1.25rem",
			"fontWeight": {
				"regular": 400,
				"bold": 700
			},
			"lineHeight": {
				"one": 1,
				"heading": 1.2,
				"medium": 1.3,
				"body": 1.5
			},
			"spacing": {
				"outer": "30px"
			}
		},
    "styles": {
      "spacing":{
        "blockGap": "30px",
        "margin": {
          "top": "0px",
          "right": "0px",
          "bottom": "0px",
          "left": "0px"
        }
      },
      "typography": {
        "fontFamily": "var(--wp--preset--font-family--playfair-display)",
        "fontSize": "var(--wp--preset--font-size--regular)",
        "fontWeight": "var(--wp--custom--font-weight--regular)",
        "lineHeight": "var(--wp--custom--line-height--body)"

      },
      "elements": {
        "button": {
          "border": {
            "color": "var(--wp--preset--color--black)",
            "radius": "0",
            "style": "solid",
            "width": "1px"
          },
          "color": {
            "background": "var(--wp--preset--color--black)",
            "text": "var(--wp--preset--color--white)"
          },
          "spacing": {
            "padding": {
              "bottom": "10px",
              "left": "25px",
              "right": "25px",
              "top": "10px"
            }
          },
          "typography": {
            "fontSize": "var(--wp--preset--font-size--regular)",
            "fontWeight": "var(--wp--custom--font-weight--regular)"
          },
          ":hover": {
            "color": {
              "background": "var(--wp--preset--color--white)",
              "text": "var(--wp--preset--color--black)"
            }
          }
        },
        "caption": {
          "color": {
            "text": "var(--wp--preset--color--black)"
          },
          "spacing": {
            "margin": {
              "top": "10px",
              "bottom": "0px"
            }
          },
          "typography": {
            "fontSize": "var(--wp--preset--font-size--medium)"
          }
        },
        "link": {
          "color": {
            "text": "var(--wp--preset--color--grey)"
          },
          ":hover": {
            "color": {
              "text": "var(--wp--preset--color--dark-grey)"
            }
          },
          ":active": {
            "color": {
              "text": "var(--wp--preset--color--grey)"
            }
          }
        },
        "h1": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--max-50)",
            "lineHeight": "var(--wp--custom--line-height--heading)",
            "fontWeight": "var(--wp--custom--font-weight--bold)",
            "fontFamily": "var(--wp--preset--font-family--roboto-slab)"
          }
        },
        "h2": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--max-40)",
            "lineHeight": "var(--wp--custom--line-height--heading)",
            "fontWeight": "var(--wp--custom--font-weight--bold)",
            "fontFamily": "var(--wp--preset--font-family--roboto-slab)"
          }
        },
        "h3": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--max-30)",
            "lineHeight": "var(--wp--custom--line-height--heading)",
            "fontWeight": "var(--wp--custom--font-weight--bold)",
            "fontFamily": "var(--wp--preset--font-family--roboto-slab)"
          }
        },
        "h4": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--xlarge)",
            "lineHeight": "var(--wp--custom--line-height--heading)",
            "fontWeight": "var(--wp--custom--font-weight--bold)",
            "fontFamily": "var(--wp--preset--font-family--roboto-slab)"
          }
        },
        "h5": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--large)",
            "lineHeight": "var(--wp--custom--line-height--heading)",
            "fontWeight": "var(--wp--custom--font-weight--bold)",
            "fontFamily": "var(--wp--preset--font-family--roboto-slab)"
          }
        },
        "h6": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--medium)",
            "lineHeight": "var(--wp--custom--line-height--heading)",
            "fontWeight": "var(--wp--custom--font-weight--bold)",
            "fontFamily": "var(--wp--preset--font-family--roboto-slab)"
          }
        }
      },
