            "padding": {
              "top": "10px",
              "right": "25px",
              "bottom": "10px",
              "left": "25px"
            }
          }
        },
        "core/calendar": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--regular)"
          }
        },
        "core/code": {
          "border": {
            "radius": "0"
          },
          "color": {
            "background": "var(--wp--preset--color--black)",
            "text": "var(--wp--preset--color--white)"
          },
          "spacing": {
            "padding": {
              "top": "25px",
              "right": "30px",
              "bottom": "25px",
              "left": "30px"
            }
          },
          "typography": {
            "fontSize": "var(--wp--preset--font-size--regular)"
          }
        },
        "core/columns": {
