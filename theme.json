        },
        "core/pullquote": {
          "border": {
            "color": "var(--wp--preset--color--black)",
            "style": "solid",
            "width": "1px"
          },
          "spacing": {
            "padding": {
              "top": "30px",
              "right": "30px",
              "bottom": "30px",
              "left": "30px"
            }
          }
        },
        "core/post-navigation-link": {
          "spacing": {
            "margin": {
              "top": "calc(var(--wp--custom--v-spacing) * 3)",
              "bottom": "var(--wp--custom--v-spacing)"
            }
          }
        },
        "core/query-pagination": {
          "spacing": {
            "margin": {
              "top": "calc(var(--wp--custom--v-spacing) * 3) !important"
            }
          }
        },
        "core/quote": {
          "border": {
            "color": "var(--wp--preset--color--black)",
            "style": "solid !important",
            "width": "1px !important"
          },
          "spacing": {
            "margin": {
              "bottom": "35px"
            },
            "padding": {
              "top": "30px",
              "right": "40px !important",
              "bottom": "30px",
              "left": "40px !important"
            }
          },
          "typography": {
            "lineHeight": "var(--wp--custom--line-height--heading)"
          }
        },
        "core/spacer": {
          "spacing": {
            "margin": {
              "top": "0 !important"
            }
          }
        },
        "core/separator": {
          "color": {
            "text": "var(--wp--preset--color--dark-grey)"
          },
          "typography": {
            "fontSize": "var(--wp--preset--font-size--large)"
          }
        },
        "core/post-author": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--regular)"
          },
          "color": {
            "text": "var(--wp--preset--color--black)"
          },
          "elements": {
            "link": {
              "color": {
                "text": "var(--wp--preset--color--dark-grey)"
              }
            }
          }
        },
        "core/post-date": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--regular)"
          },
          "color": {
            "text": "var(--wp--preset--color--black)"
          },
          "elements": {
            "link": {
              "color": {
                "text": "var(--wp--preset--color--dark-grey)"
              }
            }
          }
        },
        "core/post-title": {
          "typography": {
            "fontFamily": "var(--wp--preset--font-family--roboto-slab)"
          }
        },
        "core/post-terms": {
          "typography": {
            "fontSize": "var(--wp--preset--font-size--regular)"
          },
          "color": {
            "text": "var(--wp--preset--color--black)"
          },
          "elements": {
            "link": {
              "color": {
